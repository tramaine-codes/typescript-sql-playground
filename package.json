    "@types/better-sqlite3": "7.6.13",
    "@types/node": "24.10.9",
    "@vitest/coverage-v8": "4.0.18",
    "del-cli": "7.0.0",
    "drizzle-kit": "0.31.8",
    "eslint": "9.39.2",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "prisma": "7.3.0",
    "release-it": "19.2.4",
    "testdouble": "3.20.2",
    "tsx": "4.21.0",
